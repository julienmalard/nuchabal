{
  "name": "nuchabal",
  "version": "3.1.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "copierDonnées": "rm -r -f ruxeel/retamabäl && cp -R ../retamabäl ruxeel",
    "nettoyer": "rimraf dist",
    "compiler": "pnpm copierDonnées && pnpm versioner && pnpm nettoyer && pnpm ttsc -p tsconfig.build.json",
    "test": "pnpm copierDonnées && node --no-warnings --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand",
    "couverture": "pnpm test -- --coverage",
    "formatter": "pnpm prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\" && pnpm eslint --ext .js,.ts ruxeel",
    "publier": "pnpm compiler && (git commit -a -m 'version' || true) && pnpm publish",
    "versioner": "pnpm genversion  --es6 --semi ruxeel/relesaxïk.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julienmalard/ennikkai.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/julienmalard/nuchabal/issues"
  },
  "homepage": "https://github.com/julienmalard/nuchabal#readme",
  "devDependencies": {
    "@constl/ipa": "^0.6.30",
    "@types/jest": "^29.4.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "genversion": "^3.1.1",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "jest": {
    "transform": {
      "(.*).ts$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "preset": "ts-jest/presets/default-esm",
    "moduleNameMapper": {
      "^@/(.*).js$": "<rootDir>/ruxeel/$1",
      "^@/(.*)$": "<rootDir>/ruxeel/$1"
    },
    "testMatch": [
      "**/*.tjt.ts"
    ],
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "@constl/ipa": "^0.6.7"
  },
  "dependencies": {
    "@lassi-js/kili": "^0.0.4",
    "deepmerge": "^4.3.0",
    "events": "^3.3.0"
  }
}
